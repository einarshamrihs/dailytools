{
  "version": 2,
  "builds": [
      {
          "src": "*.py",
          "use": "@vercel/python"
      },
      {
          "src": "templates/node_modules/*",
          "use": "@vercel/static"
      }
  ],
  "routes": [
      {
          "src": "(.*)",
          "dest": "app.py"
      }
  ]
}
